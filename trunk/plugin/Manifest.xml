<?xml version="1.0" encoding="utf-8"?>
<manifest>
	<plugin name="MyPhotonCloud" version="1.0.0" s3dx_sdk_version="0x1090200">

		<info>
			<description>Plugin to use Photon Cloud server</description>
			<author>
				<name>Juan Belón Pérez</name>
				<mail>info@xixgames.com</mail>
				<site>http://xixgames.com</site>
			</author>
		</info>

		<certificate></certificate>

		<files>
			<platform type="Windows">
				<arch type="x86_32">MyPhotonCloud_Windows.dll</arch>
			</platform>

			<platform type="WinRT">
				<arch type="x86_32">MyPhotonCloud_WinRT_x86_32.dll</arch>
				<arch type="x86_64">MyPhotonCloud_WinRT_x86_64.dll</arch>
				<arch type="arm_32">MyPhotonCloud_WinRT_arm_32.dll</arch>
			</platform>

			<platform type="WinRT" toolset="wp8">
				<arch type="x86_32">MyPhotonCloud_WinRT_wp8_x86_32.dll</arch>
				<arch type="arm_32">MyPhotonCloud_WinRT_wp8_arm_32.dll</arch>
			</platform>

			<platform type="Mac">
				<arch type="ppc_32">MyPhotonCloud_Mac.dylib</arch>
				<arch type="ppc_64">MyPhotonCloud_Mac.dylib</arch>
				<arch type="x86_32">MyPhotonCloud_Mac.dylib</arch>
				<arch type="x86_64">MyPhotonCloud_Mac.dylib</arch>
			</platform>

			<platform type="Linux">
				<arch type="x86_32">MyPhotonCloud_Linux.so</arch>
			</platform>

			<platform type="iOS">
				<arch type="arm_v6">MyPhotonCloud_iOS.a</arch>
				<arch type="arm_v7">MyPhotonCloud_iOS.a</arch>
			</platform>

			<platform type="Android">
				<arch type="arm_v5te">MyPhotonCloud_Android.a</arch>
			</platform>

			<platform type="Palm">
				<arch type="arm_v6">MyPhotonCloud_Palm.a</arch>
			</platform>

			<platform type="BlackBerry">
				<arch type="arm_v7">MyPhotonCloud_BlackBerry.a</arch>
			</platform>

			<platform type="Flash">
				<arch type="noarch">MyPhotonCloud_Flash.swc</arch>
			</platform>

			<platform type="Wii">
				<arch type="ppc_32">MyPhotonCloud_Wii.a</arch>
			</platform>

			<platform type="PS3">
				<arch type="ppc_32">MyPhotonCloud_PS3.a</arch>
			</platform>

			<platform type="NaCl">
				<arch type="x86_32">MyPhotonCloud_NaCl_x86_32.a</arch>
				<arch type="x86_64">MyPhotonCloud_NaCl_x86_64.a</arch>
			</platform>

			<platform type="XB360">
				<arch type="ppc_32">MyPhotonCloud_XB360.lib</arch>
			</platform>

		</files>
	</plugin>
</manifest>